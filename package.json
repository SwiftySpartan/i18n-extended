{
  "name": "@andrewwormald/i18n-extended",
  "version": "1.0.5",
  "description": "",
  "main": "dist/bundles/i18n-extended.umd.js",
  "scripts": {
    "cleanup": "rimraf dist/bundles dist/src dist/index.d.ts dist/index.js dist/index.js.map dist/LICENCE dist/README.md",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/i18n-extended.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/i18n-extended.umd.min.js",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwiftySpartan/i18n-extended.git"
  },
  "bin": {
    "i18n-extended": "./cli/i18n-extended-cli.js"
  },
  "author": "Andrew Wormald",
  "license": "MIT",
  "keywords": [
    "Angular",
    "CLI",
    "i18n",
    "i18n-extended",
    "i18n ts",
    "translations",
    "angular translations"
  ],
  "dependencies": {
    "@angular/core": "^6.1.0",
    "enquirer": "^2.3.0",
    "fs-extra": "^8.0.1",
    "kleur": "^3.0.3",
    "listr": "^0.14.3",
    "prompts": "^2.1.0",
    "recursive-readdir": "^2.2.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "6.0.0",
    "xml2js": "^0.4.19",
    "yargs": "^13.2.4",
    "zone.js": "^0.8.26",
    "stream": "0.0.2",
    "timers": "^0.1.1"
  },
  "devDependencies": {
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@angular/platform-browser": "^6.1.10",
    "rimraf": "^2.5.4",
    "rollup": "^0.37.0",
    "typescript": "~2.7.2",
    "uglify-js": "^2.7.5"
  },
  "bugs": {
    "url": "https://github.com/SwiftySpartan/i18n-extended/issues"
  },
  "homepage": "https://github.com/SwiftySpartan/i18n-extended#readme"
}
